(
    name: "Calculator with unit conversion (uses Qalculate! expressions)",
    description: "Syntax: = <expression>\nExample: = 10 J / (196x^2) = 4 kW/s",
    query: (
        regex: "^[=\\-0-9|(-0-9)].*",
        help: "= ",
        priority: High,
        isolate_with: "^(=).*",
    ),
    bin: (path: "calc"),
    icon: Name("x-office-spreadsheet"),
    history: false,
)
